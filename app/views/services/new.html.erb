  <h1>Post an AD</h1>

  <%= simple_form_for @service do |f|%>
    <%= f.input :title %>
    <%= f.input :description %>
    <%= f.input :craft %>
    <%= f.input :volunteer, label: "Volunteer", as: :boolean %>
    <%= f.input :price %>
    <%= f.input :address %>
    <%= f.submit class: "btn btn-primary" %>
  <%end%>
